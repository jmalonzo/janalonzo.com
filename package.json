{
  "name": "janalonzo.com",
  "version": "3.1.0",
  "private": true,
  "devDependencies": {
    "html-minifier": "^3.5.2"
  },
  "dependencies": {},
  "scripts": {
    "clean": "rm -rf public static build tmp",
    "html-minifier": "html-minifier --remove-comments --collapse-whitespace --input-dir public",
    "copy-assets": "cp assets/* public/",
    "build": "npm run clean && hugo --uglyURLs='true' --theme black-and-light --verbose && npm run copy-assets",
    "start": "hugo server --config config.dev.toml --buildDrafts --theme black-and-light --verbose"
  }
}
